<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="tasa.registroeventospesca.controller.Dialog4" xmlns="sap.m"
	xmlns:form="sap.ui.layout.form">
	<Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Nuevo Evento" type="Standard"
		verticalScrolling="true">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title/>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Eventos" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<ComboBox editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%">
										<items>
											<core:Item text="Zarpe" enabled="true" key="Zarpe"/>
											<core:Item text="Llegada a Zona de Pesca" enabled="true" key="Llegada a Zona de Pesca"/>
											<core:Item text="Cala" enabled="true" key="Cala"/>
											<core:Item text="Salida de Zona de Pesca" enabled="true" key="Salida de Zona de Pesca"/>
											<core:Item text="Siniestro" enabled="true" key="Siniestro"/>
										</items>
									</ComboBox>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<beginButton/>
		<buttons>
			<Button text="OK" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
			<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="_onButtonPress1"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>